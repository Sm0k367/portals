{
  "version": 2,
  "headers": [
    {
      "source": "/src/audio/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/documents/(.*)",
      "dest": "/portal_documents/$1"
    }
  ]
}
